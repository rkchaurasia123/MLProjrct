import pandas as pd
import numpy as np
df = pd.read_csv('final_dataset.csv')

df.isnull().sum()
print(df[['budget', 'Votes', 'Duration']].dtypes)
df.fillna(method = 'ffill', inplace  = True)

from sklearn.preprocessing import OneHotEncoder
df = pd.get_dummies(df, columns=['genres', 'Languages'], drop_first=True)

print(df[['budget', 'Votes', 'Duration']].dtypes)
